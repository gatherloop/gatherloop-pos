{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project web --web",
  "targets": {
    "dev": {
      "dependsOn": [
        {
          "projects": "api-contract",
          "target": "generate:ts"
        }
      ]
    },
    "build": {
      "dependsOn": [
        {
          "projects": "api-contract",
          "target": "generate:ts"
        }
      ]
    },
    "image:build": {
      "command": "docker build -t gatherloop/gatherloop-pos-web -f apps/web/Dockerfile ."
    },
    "image:push": {
      "command": "docker push gatherloop/gatherloop-pos-web"
    }
  }
}
